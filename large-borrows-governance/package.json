{
  "name": "forta-ae-large-borrows-governance",
  "version": "0.0.1",
  "chainIds": [
    1
  ],
  "description": "This bot monitors all borrow events of COMP to see if an address has borrowed has enough COMP to influence governance.",
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "nodemon --watch src --watch forta.config.json -e js,json --exec 'forta-agent run'",
    "start:prod": "forta-agent run --prod",
    "tx": "forta-agent run --tx",
    "block": "forta-agent run --block",
    "range": "forta-agent run --range",
    "file": "forta-agent run --file",
    "publish": "forta-agent publish",
    "push": "forta-agent push",
    "disable": "forta-agent disable",
    "enable": "forta-agent enable",
    "keyfile": "forta-agent keyfile",
    "test": "jest"
  },
  "dependencies": {
    "bignumber.js": "^9.0.2",
    "forta-agent": "^0.0.39"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.25.1",
    "jest": "^27.2.5",
    "nodemon": "^2.0.16"
  }
}
