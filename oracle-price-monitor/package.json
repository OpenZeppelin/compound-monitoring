{
  "name": "forta-ae-oracle-price-monitor",
  "version": "0.0.2",
  "chainIds": [
    1
  ],
  "description": "This bot monitors the UniswapAnchoredView contract to see if any price updates have been rejected.",
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "nodemon --watch src --watch forta.config.json -e js,json --exec 'forta-agent run'",
    "start:prod": "forta-agent run --prod",
    "tx": "forta-agent run --tx",
    "block": "forta-agent run --block",
    "range": "forta-agent run --range",
    "file": "forta-agent run --file",
    "publish": "forta-agent publish",
    "push": "forta-agent push",
    "disable": "forta-agent disable",
    "enable": "forta-agent enable",
    "keyfile": "forta-agent keyfile",
    "test": "jest"
  },
  "dependencies": {
    "forta-agent": "^0.0.38",
    "axios": "^0.26.1",
    "bignumber.js": "^9.0.2",
    "dotenv": "^16.0.0",
    "ethers": "^5.6.4"
  },
  "devDependencies": {
    "eslint": "^8.16.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^28.1.0",
    "nodemon": "^2.0.8"
  }
}
