{
  "name": "forta-ae-oracle-price-monitor",
  "version": "0.0.1",
  "chainIds": [
    1
  ],
  "description": "This agent monitors the UniswapAnchoredView contract to see if any price updates have been rejected.",
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "nodemon --watch src --watch forta.config.json -e js,json --exec 'forta-agent run'",
    "start:prod": "forta-agent run --prod",
    "tx": "forta-agent run --tx",
    "block": "forta-agent run --block",
    "range": "forta-agent run --range",
    "file": "forta-agent run --file",
    "publish": "forta-agent publish",
    "push": "forta-agent push",
    "disable": "forta-agent disable",
    "enable": "forta-agent enable",
    "keyfile": "forta-agent keyfile",
    "test": "jest",
    "build": "rollup -c"
  },
  "dependencies": {
    "forta-agent": "^0.0.38",
    "axios": "^0.26.1",
    "bignumber.js": "^9.0.2",
    "dotenv": "^16.0.0",
    "ethers": "^5.6.4",
    "needle": "^3.1.0",
    "defender-relay-client": "^1.19.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "builtin-modules": "^3.1.0",
    "rollup": "^2.33.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.25.1",
    "jest": "^27.2.5",
    "nodemon": "^2.0.8"
  }
}
