{
  "name": "forta-compound-finance-liquidation-monitor",
  "version": "0.0.1",
  "description": "Forta Bot that monitors Compound Finance for positions that can be liquidated",
  "chainIds": [
    1
  ],
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "nodemon --watch src --watch forta.config.json -e js,json --exec \"forta-agent run\"",
    "start:prod": "forta-agent run --prod",
    "tx": "forta-agent run --tx",
    "block": "forta-agent run --block",
    "range": "forta-agent run --range",
    "file": "forta-agent run --file",
    "publish": "forta-agent publish",
    "push": "forta-agent push",
    "disable": "forta-agent disable",
    "enable": "forta-agent enable",
    "keyfile": "forta-agent keyfile",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "bignumber.js": "^9.0.2",
    "defender-relay-client": "^1.21.3",
    "dotenv": "^16.0.1",
    "ethers": "^5.6.8",
    "forta-agent": "^0.1.3",
    "node-fetch-commonjs": "^3.1.1"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.25.1",
    "jest": "^27.0.6",
    "nodemon": "^2.0.8"
  }
}
