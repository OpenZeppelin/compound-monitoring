{
  "name": "gnosis-safe-deploy",
  "version": "1.0.0",
  "description": "Example deployment of a Forta Bot using a Gnosis Safe smart contract.",
  "chainIds": [
    137
  ],
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "nodemon --watch src --watch forta.config.json -e js,json --exec 'forta-agent run'",
    "start:prod": "forta-agent run --prod",
    "tx": "forta-agent run --tx",
    "block": "forta-agent run --block",
    "range": "forta-agent run --range",
    "file": "forta-agent run --file",
    "test": "jest",
    "propose": "node scripts/proposeFortaBot.js",
    "push": "forta-agent push"
  },
  "dependencies": {
    "@gnosis.pm/safe-core-sdk": "^2.0.0",
    "@gnosis.pm/safe-ethers-lib": "^1.1.0",
    "@gnosis.pm/safe-service-client": "^1.1.2",
    "bignumber.js": "^9.0.2",
    "dotenv": "^16.0.0",
    "ethers": "^5.6.2",
    "forta-agent": "^0.1.3"
  },
  "devDependencies": {
    "defender-admin-client": "^1.24.0",
    "eslint": "^8.16.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^28.1.0",
    "nodemon": "^2.0.8"
  }
}
